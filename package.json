    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.258",
    "@types/node": "^20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "sidePanel",
      "storage",
      "tabs",
      "clipboardWrite"
    ],
    "side_panel": {
